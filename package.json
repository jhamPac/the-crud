    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "apollo-boost": "^0.1.23",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",

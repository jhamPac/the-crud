    "react": "^16.8.1",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.1",
    "react-testing-library": "^5.6.1",
    "rimraf": "^2.6.2",
    "yup": "^0.26.6"

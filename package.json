{
  "name": "the-crud",
  "version": "0.1.0",
  "description": "Basic crud with GraphQL",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist/*",
    "bablify": "parcel build ./functions/source.js --out-dir ./functions --out-file index.js --target node",
    "build": "npm run clean && npm run bablify && parcel build ./src/index.html",
    "start": "parcel ./src/index.html --port 9500",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhamPac/the-crud.git"
  },
  "author": "jhampac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhamPac/the-crud/issues"
  },
  "homepage": "https://github.com/jhamPac/the-crud#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
